<%const title = "Un blog";%>
<%- include('head.ejs', {title}); -%>

<body>
    <%- include('nav.ejs'); -%>
    <main>
        <% indexData.articles.forEach(function(article) { %>
        <article>
            <header>
                <h2><%- article.title %></h2>
            </header>
            <section>
                <%- article.content %>
            </section>
            <footer>
                By <b><%- article.author %></b>
            </footer>
        </article>
        <hr>
        <% }); %>
        <footer>
            &copy; Ejemplo para incluir la mayor cantidad posible de etiquetas HTML
        </footer>
    </main>
</body>
</html>
